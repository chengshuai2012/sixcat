<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.link.cloud">



    <!-- 调用摄像头功能 -->
    <uses-permission android:name="android.permission.CAMERA" />
    <!-- 自动对焦功能 -->
    <uses-feature android:name="android.hardware.camera" />
    <!-- 调用前置摄像头功能 -->
    <uses-feature android:name="android.hardware.camera.front" />


    <application
        android:name=".SixCatApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.BindActivity" />
        <activity android:name=".activity.EliminateClassActivity" />
        <activity android:name=".activity.ChoiceWayActivity" />
        <activity android:name=".activity.AttendClassActivity" />


        <service android:name=".veune.MdUsbService"/>

    </application>

</manifest>